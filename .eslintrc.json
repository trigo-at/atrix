{
    "extends": [
        "trigo",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
		"strict": [2, "global"],
		"no-underscore-dangle": 0,
        "prettier/prettier": ["error"]
    }
}
